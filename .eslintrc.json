{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "globalReturn": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/prefer-default-export": "off",
    "no-unused-expressions:": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "no-console": "off",
    "import/no-unresolved": "off"
  }
}
