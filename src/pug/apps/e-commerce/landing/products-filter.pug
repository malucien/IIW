block ecomContent
	+Section(null,'container').pt-5.pb-9
		button.btn.btn-sm.btn-phoenix-secondary.text-700.mb-5.d-lg-none(data-phoenix-toggle='offcanvas' data-phoenix-target='#productFilterColumn') 
			span.fa-solid.fa-filter.me-2
			| Filter
