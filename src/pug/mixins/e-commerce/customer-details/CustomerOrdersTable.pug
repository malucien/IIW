mixin CustomerOrdersTableRow(data,index)
  tr.hover-actions-trigger.btn-reveal-trigger.position-static
    td.order.align-middle.white-space-nowrap.ps-0
      a.fw-semi-bold(href='#!')= `#${data.order}`
    td.total.align-middle.text-end.fw-semi-bold.pe-7.text-1000=`$${data.total}`
    td.payment_status.align-middle.white-space-nowrap.text-start.fw-bold.text-700
      span.badge.badge-phoenix.fs--2(class=data.payment_status.type)
        span.badge-label=data.payment_status.label
        span.ms-1(data-feather=data.payment_status.icon style='height:12.8px;width:12.8px;')
    td.fulfilment_status.align-middle.white-space-nowrap.text-start.fw-bold.text-700
      span.badge.badge-phoenix.fs--2(class=data.fulfilment_status.type)
        span.badge-label=data.fulfilment_status.label
        span.ms-1(data-feather=data.fulfilment_status.icon style='height:12.8px;width:12.8px;')
    td.delivery_type.align-middle.white-space-nowrap.text-900.fs--1.text-start=data.delivery_type
    td.date.align-middle.white-space-nowrap.text-700.fs--1.ps-4.text-end=data.date
    td.align-middle.white-space-nowrap.text-end.pe-0.ps-5
      +DashboardDropdown.fs--2

