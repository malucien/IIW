  #customerOrdersTable.border-top.border-bottom.border-200(data-list!={
      valueNames: ["order", "total", "payment_status", "fulfilment_status", "delivery_type", "date"],
      page: 6,
      pagination: true
    })
    .table-responsive.scrollbar
      table.table.table-sm.fs--1.mb-0
        thead
          tr
            th.sort.white-space-nowrap.align-middle.ps-0.pe-3(scope='col' data-sort="order" style='width:10%;') ORDER
            th.sort.align-middle.text-end.pe-7(scope='col' data-sort="total" style='width:10%;') TOTAL
            th.sort.align-middle.white-space-nowrap.pe-3(scope='col' data-sort="payment_status" style='width:15%;') PAYMENT STATUS
            th.sort.align-middle.white-space-nowrap.text-start.pe-3(scope='col' data-sort="fulfilment_status" style='width:20%;') FULFILMENT STATUS
            th.sort.align-middle.white-space-nowrap.text-start(scope='col' data-sort="delivery_type"  style='width:30%;') DELIVERY TYPE
            th.sort.align-middle.text-end.pe-0(scope='col' data-sort="date") DATE
            th.sort.text-end.align-middle.pe-0.ps-5(scope='col')
             
        tbody.list#customer-order-table-body
          each row,index in customerOrdersTableData
            +CustomerOrdersTableRow(row,index)
    .row.align-items-center.justify-content-between.py-2.pe-0.fs--1
      .col-auto.d-flex
