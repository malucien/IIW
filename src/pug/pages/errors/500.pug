extends ../../layouts/LayoutTheme

block wrapperContent
  .px-3
    .row.min-vh-100.flex-center.p-5
      .col-12.col-xl-10.col-xxl-8
        .row.justify-content-center.g-5
          .col-12.col-lg-6.text-center.order-lg-1
            img.img-fluid.w-lg-100.d-light-none(src=`${CWD}assets/img/spot-illustrations/500-illustration.png` alt="" width=400)
            img.img-fluid.w-md-50.w-lg-100.d-dark-none(src=`${CWD}assets/img/spot-illustrations/dark_500-illustration.png` alt="" width=540)
          .col-12.col-lg-6.text-center.text-lg-start
            img.img-fluid.mb-6.w-50.w-lg-75.d-dark-none(src=`${CWD}assets/img/spot-illustrations/500.png` alt="")
            img.img-fluid.mb-6.w-50.w-lg-75.d-light-none(src=`${CWD}assets/img/spot-illustrations/dark_500.png` alt="")
            h2.text-800.fw-bolder.mb-3 Unknow error!
            p.text-900.mb-5 But relax! Our cat is here to play you some music.
            a.btn.btn-lg.btn-primary(href=`${CWD}${paths['default-dashboard']}`) Go Home